{
  "name": "checkmein",
  "version": "1.0.0",
  "description": "A check-in app powered by backand",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3000  --content-base src",
    "start": "npm run server"
  },
  "contributors": [
    "Rachata Kampitak <opkampitak@hotmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "~0.16.2",
    "css-loader": "^0.23.1",
    "es6-promise": "3.0.2",
    "es6-shim": "0.35.0",
    "file-loader": "^0.8.5",
    "reflect-metadata": "0.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "typescript": "~1.8.9",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/platform-server": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "bootstrap": "^3.3.6",
    "core-js": "^2.2.0",
    "jquery": "^2.2.4",
    "leaflet": "^0.7.7",
    "rxjs": "5.0.0-beta.6",
    "typings": "^1.0.4",
    "zone.js": "~0.6.12"
  },
  "keywords": [
    "angular2",
    "backand",
    "leaflet",
    "checkin",
    "checkmein"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/op01/checkmein.git"
  },
